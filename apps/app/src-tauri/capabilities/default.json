{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "os:default",
    "process:default",
    "core:window:allow-maximize",
    "core:window:allow-unmaximize",
    "core:window:allow-minimize"
  ]
}
